<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.educode.examentottus">

    <application
        android:name=".TottusApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ExamenTottus">
        <activity android:name=".presentation.ui.home.ui.team.dialog.DialogFragment"></activity>
        <activity
            android:name=".presentation.ui.home.HomeActivity"
            android:label="@string/title_activity_home"
            android:theme="@style/Theme.ExamenTottus.NoActionBar"></activity>
        <activity android:name=".presentation.ui.register.RegisterActivity" />
        <activity
            android:name=".presentation.ui.login.LoginActivity"
            android:theme="@style/Theme.Login" />
        <activity
            android:name=".presentation.ui.splash.SplashActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".presentation.ui.MainActivity" />
    </application>

</manifest>